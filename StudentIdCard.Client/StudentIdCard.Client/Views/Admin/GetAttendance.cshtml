@model IEnumerable<StudentIdCard.Client.Models.Attendance>

@{
    ViewData["Title"] = "ProductList";
}

<div class="mdl-cell mdl-cell--12-col-desktop mdl-cell--12-col-tablet mdl-cell--4-col-phone ">
    <table class="mdl-data-table mdl-js-data-table mdl-data-table--selectable mdl-shadow--2dp projects-table">
        <thead>
            <tr>
                <th class="mdl-data-table__cell--non-numeric">Öğrenci Adı</th>
                <th class="mdl-data-table__cell--non-numeric">Giriş Saati</th>
                <th class="mdl-data-table__cell--non-numeric">Çıkış Saati</th>

            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {

                <tr>
                    <td class="mdl-data-table__cell--non-numeric">@item.CardName @item.CardSurname</td>
                    <td class="mdl-data-table__cell--non-numeric">@Convert.ToDateTime(item.Entry)</td>
                    <td class="mdl-data-table__cell--non-numeric">@Convert.ToDateTime(item.Leave)</td>

                </tr>
            }


        </tbody>
    </table>
</div>
